{
  "name": "mysql-sql-unner",
  "description": "MySql SQL Query  Runner GitHub Action",
  "version": "0.13.0",
  "homepage": "https://readtheoty.org",
  "license": "Apache-2.0",
  "main": "lib/main.js",
  "scripts": {
    "start": "ts-node src/main.ts",
    "build": "tsc && ncc build -o dist/action src/main.ts",
    "test": "tsc --noEmit && jest --coverage",
